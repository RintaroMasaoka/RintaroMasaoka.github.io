---
import BaseLayout from '../layouts/BaseLayout.astro';
import HomeContent from '../components/HomeContent.astro';
import { localize } from '../i18n/utils';
import { profile } from '../utils/data';

const lang = 'en';
const pageData = profile.page;
---

<BaseLayout title={`${localize(profile.name, lang)} - ${pageData.title[lang]}`} lang={lang}>
  <HomeContent lang={lang} />
</BaseLayout>

